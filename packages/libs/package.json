    "@nextui-org/spinner": "^2.0.30",
    "@nextui-org/switch": "^2.0.31",
    "@nextui-org/system": "^2.2.2",
    "@nextui-org/table": "^2.0.36",
    "@nextui-org/tabs": "^2.0.32",
    "@nextui-org/theme": "^2.2.6",
    "@nextui-org/user": "^2.0.31",
    "@supabase/ssr": "^0.3.0",
    "dayjs": "^1.11.11",
    "framer-motion": "^11.2.11",
    "gql.tada": "^1.7.6",
