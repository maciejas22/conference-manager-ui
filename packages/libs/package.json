{
  "name": "@repo/libs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "gen-gql-output": "gql.tada generate output",
    "gen-gql-turbo": "gql.tada generate turbo",
    "gen-gql-persisted": "gql.tada generate persisted",
    "gen-gql": "bun gen-gql-output && bun gen-gql-turbo && bun gen-gql-persisted",
    "gen-tailwind": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "gen": "bun gen-gql&& bun gen-tailwind",
    "dev": "bun gen && bun gen-tailwind --watch",
    "build": "bun gen"
  },
  "exports": {
    "./dayjs": "./src/dayjs/dayjs.ts",
    "./graphql": "./src/graphql/graphql.ts",
    "./graphql-client": "./src/graphql-client/index.ts",
    "./nextui": "./src/nextui/index.ts",
    "./supabase/client": "./src/supabase/client.ts",
    "./supabase/middleware": "./src/supabase/middleware.ts",
    "./styles": "./dist/index.css"
  },
  "dependencies": {
    "@nextui-org/accordion": "^2.0.35",
    "@nextui-org/button": "^2.0.34",
    "@nextui-org/card": "^2.0.31",
    "@nextui-org/chip": "^2.0.30",
    "@nextui-org/date-input": "^2.1.1",
    "@nextui-org/divider": "^2.0.28",
    "@nextui-org/dropdown": "^2.1.26",
    "@nextui-org/input": "^2.2.2",
    "@nextui-org/link": "^2.0.32",
    "@nextui-org/navbar": "^2.0.33",
