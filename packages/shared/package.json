{
  "name": "@repo/shared",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./browser-fetcher": "./src/utils/fetchers/browser-fetcher.ts",
    "./components": "./src/components/index.ts",
    "./layouts": "./src/layouts/index.ts",
    "./graphql": "./src/libs/graphql/graphql.ts",
    "./graphql-client": "./src/libs/graphql-client/index.ts",
    "./nextui": "./src/libs/nextui/index.ts",
    "./server-fetcher": "./src/utils/fetchers/server-fetcher.ts",
    "./supabase/actions": "./src/libs/supabase/actions.ts",
    "./supabase/browser-client": "./src/libs/supabase/browser-client.ts",
    "./supabase/middleware": "./src/libs/supabase/middleware.ts",
    "./supabase/server-client": "./src/libs/supabase/server-client.ts",
    "./styles": "./dist/index.css",
    "./utils": "./src/utils/index.ts"
  },
  "scripts": {
    "build": "pnpm gen-tailwind",
    "clean": "rm -rf dist node_modules",
    "dev": "pnpm gen && pnpm gen-tailwind --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "gen": "pnpm gen-gql && pnpm gen-tailwind",
    "gen-gql": "pnpm gen-schema && pnpm gen-types",
    "gen-gql-output": "gql.tada generate output",
    "gen-gql-persisted": "gql.tada generate persisted",
    "gen-gql-turbo": "gql.tada generate turbo",
    "gen-schema": "gql.tada generate schema 'http://localhost:8080/graphql' --output ./src/libs/graphql/schema.graphql",
    "gen-tailwind": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "gen-types": "pnpm prepare-gql-directories && pnpm gen-gql-output && pnpm gen-gql-turbo && pnpm gen-gql-persisted",
    "gql-check": "gql.tada check",
    "lint": "eslint src",
    "prepare-gql-directories": "mkdir -p ./src/libs/graphql/types",
    "prettier-check": "prettier --check \"src/**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "type-check": "pnpm gql-check && tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@internationalized/date": "^3.5.5",
    "@nextui-org/accordion": "^2.0.38",
    "@nextui-org/button": "^2.0.37",
    "@nextui-org/card": "^2.0.33",
    "@nextui-org/chip": "^2.0.32",
    "@nextui-org/date-input": "^2.1.3",
    "@nextui-org/date-picker": "^2.1.6",
    "@nextui-org/divider": "^2.0.31",
    "@nextui-org/dropdown": "^2.1.29",
    "@nextui-org/input": "^2.2.4",
    "@nextui-org/link": "^2.0.34",
    "@nextui-org/navbar": "^2.0.36",
    "@nextui-org/pagination": "^2.0.35",
    "@nextui-org/popover": "^2.1.27",
    "@nextui-org/radio": "^2.1.4",
    "@nextui-org/select": "^2.2.5",
    "@nextui-org/spinner": "^2.0.33",
    "@nextui-org/switch": "^2.0.33",
    "@nextui-org/system": "^2.2.3",
    "@nextui-org/table": "^2.0.39",
    "@nextui-org/tabs": "^2.0.35",
    "@nextui-org/theme": "^2.2.6",
    "@nextui-org/tooltip": "^2.0.39",
    "@nextui-org/user": "^2.0.33",
    "@supabase/ssr": "^0.4.1",
    "@supabase/supabase-js": "^2.45.1",
    "framer-motion": "^11.2.11",
    "gql.tada": "^1.8.5",
    "graphql": "^16.9.0",
    "graphql-request": "^7.0.1",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@iconify/react": "^5.0.1",
    "@repo/config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8",
    "nextui-cli": "^0.3.2",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "imports": {
    "#assets/*": "./src/assets/*",
    "#components/*": "./src/components/*",
    "#libs/*": "./src/libs/*",
    "#utils/*": "./src/utils/*"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
