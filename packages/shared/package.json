{
  "name": "@repo/shared",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./graphql": "./src/graphql/graphql.ts",
    "./graphql-client": "./src/graphql-client/index.ts",
    "./nextui": "./src/nextui/index.ts",
    "./supabase/client": "./src/supabase/client.ts",
    "./supabase/middleware": "./src/supabase/middleware.ts",
    "./styles": "./dist/index.css",
    "./utils/formatters": "./src/utils/formatters/index.ts"
  },
  "scripts": {
    "build": "pnpm gen-tailwind",
    "clean": "rm -rf dist node_modules",
    "dev": "pnpm gen && pnpm gen-tailwind --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "gen": "pnpm gen-gql && pnpm gen-tailwind",
    "gen-gql": "pnpm gen-schema && pnpm gen-types",
    "gen-gql-output": "gql.tada generate output",
    "gen-gql-persisted": "gql.tada generate persisted",
    "gen-gql-turbo": "gql.tada generate turbo",
    "gen-schema": "gql.tada generate schema 'http://localhost:8080/graphql' --output ./src/graphql/schema.graphql",
    "gen-tailwind": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "gen-types": "pnpm prepare-gql-directories && pnpm gen-gql-output && pnpm gen-gql-turbo && pnpm gen-gql-persisted",
    "gql-check": "gql.tada check",
    "lint": "eslint src",
    "prepare-gql-directories": "mkdir -p ./src/graphql/types",
    "prettier-check": "prettier --check \"src/**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "type-check": "pnpm gql-check && tsc --noEmit"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@nextui-org/accordion": "^2.0.38",
    "@nextui-org/button": "^2.0.35",
    "@nextui-org/card": "^2.0.31",
    "@nextui-org/chip": "^2.0.30",
    "@nextui-org/date-input": "^2.1.1",
    "@nextui-org/date-picker": "^2.1.5",
    "@nextui-org/divider": "^2.0.28",
    "@nextui-org/dropdown": "^2.1.26",
    "@nextui-org/input": "^2.2.2",
    "@nextui-org/link": "^2.0.32",
    "@nextui-org/navbar": "^2.0.33",
    "@nextui-org/pagination": "^2.0.33",
    "@nextui-org/popover": "^2.1.25",
    "@nextui-org/radio": "^2.1.2",
    "@nextui-org/select": "^2.2.2",
    "@nextui-org/spinner": "^2.0.31",
    "@nextui-org/switch": "^2.0.31",
    "@nextui-org/system": "^2.2.3",
    "@nextui-org/table": "^2.0.36",
    "@nextui-org/tabs": "^2.0.35",
    "@nextui-org/theme": "^2.2.6",
    "@nextui-org/tooltip": "^2.0.39",
    "@nextui-org/user": "^2.0.31",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.44.4",
    "framer-motion": "^11.2.11",
    "gql.tada": "^1.8.5",
    "graphql": "^16.9.0",
    "graphql-request": "^7.0.1",
    "next": "^14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@repo/config": "workspace:*",
    "eslint": "^8",
    "nextui-cli": "^0.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.4"
  },
  "imports": {
    "#supabase/*": "./src/supabase/*.ts"
  }
}
