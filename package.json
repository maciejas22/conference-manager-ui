{
  "name": "conference-manager-ui",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules",
    "dev": "turbo dev",
    "format": "turbo format && pnpm syncpack:format",
    "gen-graph": "turbo run build --graph=graph.png",
    "gen-schema": "turbo gen-schema",
    "gen-types": "turbo gen-types",
    "lint": "turbo lint",
    "prettier-check": "turbo prettier-check",
    "syncpack:format": "syncpack format",
    "syncpack:lint": "syncpack lint",
    "syncpack:list": "syncpack list",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "syncpack": "^12.4.0",
    "turbo": "^2.0.4"
  },
  "packageManager": "pnpm@9.6.0"
}
