{
  "name": "conference-manager-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next node_modules",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "gen": "pnpm gen-gql && pnpm gen-tailwind",
    "gen-gql": "pnpm gen-schema && pnpm gen-types",
    "gen-gql-output": "gql.tada generate output",
    "gen-gql-persisted": "gql.tada generate persisted",
    "gen-gql-turbo": "gql.tada generate turbo",
    "gen-schema": "gql.tada generate schema 'http://localhost:8080/graphql' --output ./libs/graphql/schema.graphql",
    "gen-tailwind": "tailwindcss -i ./styles.css -o ./dist/index.css",
    "gen-types": "pnpm prepare-gql-directories && pnpm gen-gql-output && pnpm gen-gql-turbo && pnpm gen-gql-persisted",
    "gql-check": "gql.tada check",
    "lint": "next lint",
    "prepare-gql-directories": "mkdir -p ./libs/graphql/types",
    "prettier-check": "prettier --check \"**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@internationalized/date": "^3.5.6",
    "@next/bundle-analyzer": "^14.2.15",
    "@nextui-org/accordion": "^2.0.40",
    "@nextui-org/button": "^2.0.38",
    "@nextui-org/card": "^2.0.34",
    "@nextui-org/chip": "^2.0.33",
    "@nextui-org/date-input": "^2.1.4",
    "@nextui-org/date-picker": "^2.1.8",
    "@nextui-org/divider": "^2.0.32",
    "@nextui-org/dropdown": "^2.1.31",
    "@nextui-org/input": "^2.2.5",
    "@nextui-org/link": "^2.0.35",
    "@nextui-org/modal": "^2.0.41",
    "@nextui-org/navbar": "^2.0.37",
    "@nextui-org/pagination": "^2.0.36",
    "@nextui-org/popover": "^2.1.29",
    "@nextui-org/radio": "^2.1.5",
    "@nextui-org/select": "^2.2.7",
    "@nextui-org/skeleton": "^2.0.32",
    "@nextui-org/spinner": "^2.0.34",
    "@nextui-org/switch": "^2.0.34",
    "@nextui-org/system": "^2.2.6",
    "@nextui-org/table": "^2.0.40",
    "@nextui-org/tabs": "^2.0.37",
    "@nextui-org/theme": "^2.2.11",
    "@nextui-org/tooltip": "^2.0.41",
    "@nextui-org/user": "^2.0.34",
    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.8.0",
    "@tanstack/react-query": "^5.59.15",
    "@tanstack/react-query-devtools": "^5.59.15",
    "@tanstack/react-query-next-experimental": "^5.59.15",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "framer-motion": "^11.11.9",
    "gql.tada": "^1.8.10",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.omitby": "^4.6.0",
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.9",
    "react-hook-form": "^7.53.0",
    "recharts": "^2.13.0",
    "sonner": "^1.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@iconify/react": "^5.0.2",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vercel/style-guide": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.1.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "nextui-cli": "^0.3.4",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": "^20"
  }
}
