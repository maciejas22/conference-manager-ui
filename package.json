{
  "name": "conference-manager-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "prepare-gql-directories": "mkdir -p ./libs/graphql/types",
    "prettier-check": "prettier --check \"**/*.{ts,tsx,js,cjs,mjs,json,md}\"",
    "gen": "pnpm gen-gql && pnpm gen-tailwind",
    "gen-gql": "pnpm gen-schema && pnpm gen-types",
    "gen-gql-output": "gql.tada generate output",
    "gen-gql-persisted": "gql.tada generate persisted",
    "gen-gql-turbo": "gql.tada generate turbo",
    "gen-schema": "gql.tada generate schema 'http://localhost:8080/graphql' --output ./libs/graphql/schema.graphql",
    "gen-tailwind": "tailwindcss -i ./styles.css -o ./dist/index.css",
    "gen-types": "pnpm prepare-gql-directories && pnpm gen-gql-output && pnpm gen-gql-turbo && pnpm gen-gql-persisted",
    "gql-check": "gql.tada check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@internationalized/date": "^3.5.5",
    "@next/bundle-analyzer": "^14.2.4",
    "@nextui-org/accordion": "^2.0.38",
    "@nextui-org/button": "^2.0.37",
    "@nextui-org/card": "^2.0.33",
    "@nextui-org/chip": "^2.0.32",
    "@nextui-org/date-input": "^2.1.3",
    "@nextui-org/date-picker": "^2.1.6",
    "@nextui-org/divider": "^2.0.31",
    "@nextui-org/dropdown": "^2.1.29",
    "@nextui-org/input": "^2.2.4",
    "@nextui-org/link": "^2.0.34",
    "@nextui-org/navbar": "^2.0.36",
    "@nextui-org/pagination": "^2.0.35",
    "@nextui-org/popover": "^2.1.27",
    "@nextui-org/radio": "^2.1.4",
    "@nextui-org/select": "^2.2.5",
    "@nextui-org/skeleton": "^2.0.32",
    "@nextui-org/spinner": "^2.0.33",
    "@nextui-org/switch": "^2.0.33",
    "@nextui-org/system": "^2.2.3",
    "@nextui-org/table": "^2.0.39",
    "@nextui-org/tabs": "^2.0.35",
    "@nextui-org/theme": "^2.2.6",
    "@nextui-org/tooltip": "^2.0.39",
    "@nextui-org/user": "^2.0.33",
    "@tanstack/react-query": "^5.56.2",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "framer-motion": "^11.2.11",
    "gql.tada": "^1.8.5",
    "graphql": "^16.9.0",
    "graphql-request": "^7.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.omitby": "^4.6.0",
    "next": "14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.52.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@iconify/react": "^5.0.1",
    "@tanstack/react-query-devtools": "^5.56.2",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/style-guide": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.11",
    "eslint-plugin-only-warn": "^1.1.0",
    "nextui-cli": "^0.3.2",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.1.1"
  }
}
