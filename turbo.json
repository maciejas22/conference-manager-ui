{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["SUPABASE_URL", "SUPABASE_ANON_KEY", "ANALYZE", "API_DOMAIN"],
  "tasks": {
    "build": {
      "dependsOn": ["^gen-types", "^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "gen-schema": {
      "cache": false
    },
    "gen-types": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "prettier-check": {},
    "type-check": {
      "dependsOn": ["^gen-types"]
    }
  }
}
